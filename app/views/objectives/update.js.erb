var objectiveId = "#objective-<%= @objective.id %>";
var status = "<%= @objective.status %>";
var classBtn
switch ("<%= @objective.status %>") {
  case "Not Started":
    classBtn = "btn btn-obj-not-started btn-block";
    break;
  case "In Progress":
    classBtn = "btn btn-obj-in-progress btn-block";
    break;
  case "Completed":
    classBtn = "btn btn-obj-completed btn-block";
    break;
};

$(objectiveId).text(status).removeClass().addClass(classBtn);
